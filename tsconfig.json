{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "useUnknownInCatchVariables": false,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "lib": ["ESNext"],
    "target": "ESNext",
    "removeComments": true,
    "declaration": true,
    "types": ["vitest/globals"],
    "outDir": "./dist"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
